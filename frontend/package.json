{
  "name": "botcrud-frontend",
  "version": "1.0.0",
  "description": "BotCRUD Frontend - Quasar Application",
  "productName": "BotCRUD",
  "author": "abernardo",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,css,html,md,json}\" --ignore-path .gitignore",
    "dev": "quasar dev",
    "build": "quasar build",
    "dev:android": "quasar dev -m capacitor -T android",
    "dev:ios": "quasar dev -m capacitor -T ios",
    "build:android": "quasar build -m capacitor -T android",
    "build:ios": "quasar build -m capacitor -T ios",
    "dev:electron": "quasar dev -m electron",
    "build:electron": "quasar build -m electron"
  },
  "dependencies": {
    "@abernardo/api-client": "1.0.6",
    "@quasar/extras": "^1.16.9",
    "axios": "^1.6.2",
    "chart.js": "^4.5.1",
    "pinia": "^2.1.7",
    "quasar": "^2.14.2",
    "vue": "^3.4.18",
    "vue-chartjs": "^5.3.3",
    "vue-i18n": "^9.9.0",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@quasar/app-vite": "^1.7.3",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.19.2",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3",
    "vite-plugin-checker": "^0.6.2",
    "vue-tsc": "^1.8.27"
  },
  "engines": {
    "node": "^24 || ^22 || ^20 || ^18",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
